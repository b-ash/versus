#header-wrap 
    height: 225px
    
    #header
        background-image: url('/static/images/header_bkgd.png')
        width: 100%
        height: 150px
        float: left

    #header-graphic-wrap
        position: relative
        background-repeat: no-repeat 
        background-position: center
        width: 100%

    #header-graphic
        background: transparent url('/static/images/header_graphic.png')
        width: 950px
        height: 225px
        margin: 0 auto

        &.floating
            left: 9%
